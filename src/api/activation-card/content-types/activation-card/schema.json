{
  "kind": "collectionType",
  "collectionName": "activation_cards",
  "info": {
    "singularName": "activation-card",
    "pluralName": "activation-cards",
    "displayName": "activation-card"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "options": {
        "disable-auto-fill": false,
        "disable-regenerate": false,
        "uuid-format": "[A-Z0-9]{18}"
      }
    },
    "user_id": {
      "type": "string"
    },
    "card_type": {
      "type": "enumeration",
      "default": "test",
      "enum": [
        "test",
        "day",
        "week",
        "month",
        "year",
        "permanent"
      ]
    },
    "activation_status": {
      "type": "enumeration",
      "default": "unassigned",
      "enum": [
        "unassigned",
        "assigned",
        "used",
        "expired"
      ]
    },
    "used_at": {
      "type": "datetime"
    },
    "expires_at": {
      "type": "datetime"
    },
    "metadata": {
      "type": "json"
    },
    "note": {
      "type": "string"
    }
  }
}
